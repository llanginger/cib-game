{
    "name": "Cib",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start:ios":
            "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "start:ios_x":
            "npm run build && concurrently -r 'npm run watch' 'react-native run-ios --simulator=\"iPhone X\"'",
        "watch": "npm run build -- -w",
        "test": "jest",
        "tsc": "tsc",
        "copy-assets":
            "copyfiles -f src/images/**.* build/images && copyfiles -f src/images/laia/**.* build/images/laia",
        "clean": "rimraf build",
        "build": "npm run clean && npm run copy-assets && npm run tsc --"
    },
    "dependencies": {
        "@types/lodash": "^4.14.85",
        "@types/react": "^16.0.12",
        "@types/react-native": "^0.52.8",
        "@types/react-redux": "^5.0.11",
        "global": "^4.3.2",
        "lodash": "^4.17.4",
        "react": "16.0.0-beta.5",
        "react-native": "0.49.1",
        "react-native-animatable": "^1.2.4",
        "react-native-device-info": "^0.13.0",
        "react-native-elements": "^0.17.0",
        "react-native-gifted-chat": "^0.2.9",
        "react-native-maps": "^0.16.4",
        "react-native-navigation": "^1.1.235",
        "react-native-vector-icons": "^4.4.0",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "shortid": "^2.2.8",
        "tslint": "^5.9.1",
        "typescript": "2.7.1"
    },
    "devDependencies": {
        "babel-jest": "21.2.0",
        "babel-preset-react-native": "4.0.0",
        "concurrently": "^3.5.0",
        "jest": "21.2.1",
        "prettier": "1.11.1",
        "react-test-renderer": "16.0.0-beta.5"
    },
    "jest": {
        "preset": "react-native"
    }
}
